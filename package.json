{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.0-rc.0",
    "@babel/eslint-parser": "^7.16.5",
    "@react-native-async-storage/async-storage": "^1.15.15",
    "@react-native-picker/picker": "^2.2.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/react": "^17.0.38",
    "babel-loader": "^8.2.3",
    "crypto-js": "3.1.9-1",
    "dotenv": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^25.3.4",
    "expo": "^42.0.0",
    "expo-cli": "^5.0.3",
    "expo-status-bar": "~1.0.4",
    "formik": "^2.2.9",
    "graphql": "15.0.0",
    "jest": "^27.4.7",
    "jest-expo": "^44.0.1",
    "react": "^17.0.2",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "^0.17.5",
    "react-router-dom": "^6.2.1",
    "react-router-native": "^5.2.1",
    "react-test-renderer": "17.0.1",
    "use-debounce": "^7.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@expo/webpack-config": "^0.16.15",
    "eslint-plugin-react": "^7.28.0"
  },
  "private": true
}
